{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "standard",
  "parser": "babel-eslint",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["svelte3"],
  "rules": {
    "import/first": false
  }
}
